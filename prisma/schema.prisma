generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ConnectInstallation {
  id           Int    @id @default(autoincrement())
  key          String @unique
  clientKey    String
  sharedSecret String
  baseUrl      String
  displayUrl   String
}

model OAuthUserCredential {
  id           Int    @id @default(autoincrement())
  userId       String @unique
  accessToken  String
  refreshToken String
  expiresIn    String
}
